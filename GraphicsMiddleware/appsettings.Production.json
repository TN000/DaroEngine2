{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "AllowedHosts": "localhost;127.0.0.1",
  "Database": {
    "Path": "graphics_middleware.db"
  },
  "Templates": {
    "Path": "%USERPROFILE%\\Documents\\DaroEngine\\Templates",
    "CacheDurationSeconds": 60
  },
  "Scenes": {
    "Path": "%USERPROFILE%\\Documents\\DaroEngine\\Scenes"
  },
  "MosartClient": {
    "Host": "127.0.0.1",
    "Port": 5555,
    "CommandTimeoutMs": 1000,
    "ConnectTimeoutMs": 1000,
    "MaxRetryAttempts": 3,
    "BaseRetryDelayMs": 100
  },
  "Mosart": {
    "Port": 5555
  },
  "State": {
    "ExpirationMinutes": 30
  },
  "Cors": {
    "AllowedOrigins": []
  }
}
